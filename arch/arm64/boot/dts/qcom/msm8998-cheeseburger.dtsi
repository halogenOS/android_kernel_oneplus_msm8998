&remoteproc_adsp {
	status = "okay";
};

&remoteproc_slpi {
	status = "okay";
};

&tlmm {
	gpio-reserved-ranges = <0 4>, <81 4>;

	dsi_default: mdss_dsi_active {
		mux {
			pins = "gpio94", "gpio91", "gpio51";
			function = "gpio";
		};

		config {
			pins = "gpio94", "gpio91", "gpio51";
			drive-strength = <8>; // 8 mA
			bias-disable; // no pull
		};
	};

	dsi_sleep: mdss_dsi_suspend {
		mux {
			pins = "gpio94", "gpio91", "gpio51";
			function = "gpio";
		};

		config {
			pins = "gpio94", "gpio91", "gpio51";
			drive-strength = <2>; // 2 mA
			bias-pull-down;
		};
	};

	touch_default: touch_default {
		mux {
			pins = "gpio89", "gpio125";
			function = "gpio";
		};

		config {
			pins = "gpio89", "gpio125";
			drive-strength = <16>;
			bias-pull-up;
		};
	};

	touch_int_suspend: touch_int_suspend {
		mux {
			pins = "gpio125";
			function = "gpio";
		};

		config {
			pins = "gpio125";
			drive-strength = <2>;
			bias-disable;
		};
	};

	touch_reset_suspend: touch_reset_suspend {
		mux {
			pins = "gpio89";
			function = "gpio";
		};

		config {
			pins = "gpio89";
			drive-strength = <2>;
			bias-pull-down;
		};
	};

	blsp1_i2c5_default: blsp1_i2c5 {
		mux {
			pins = "gpio87", "gpio88";
			function = "blsp_i2c5";
		};

		config {
			pins = "gpio87", "gpio88";
			drive-strength = <2>;
			bias-disable;
		};
	};

	blsp1_i2c5_sleep: blsp1_i2c5_sleep {
		mux {
			pins = "gpio87", "gpio88";
			function = "blsp_i2c5";
		};

		config {
			pins = "gpio87", "gpio88";
			drive-strength = <2>;
			bias-disable;
		};
	};
};

&pcie0 {
	status = "disabled";
};

&ufshc {
	vcc-supply = <&vreg_l20a_2p95>;
	vccq-supply = <&vreg_l26a_1p2>;
	vccq2-supply = <&vreg_s4a_1p8>;
	vcc-max-microamp = <750000>;
	vccq-max-microamp = <560000>;
	vccq2-max-microamp = <750000>;
};

&ufsphy {
	vdda-phy-supply = <&vreg_l1a_0p875>;
	vdda-pll-supply = <&vreg_l2a_1p2>;
	vddp-ref-clk-supply = <&vreg_l26a_1p2>;
	vdda-phy-max-microamp = <51400>;
	vdda-pll-max-microamp = <14600>;
	vddp-ref-clk-max-microamp = <100>;
	vddp-ref-clk-always-on;
};

&usb3 {
	status = "okay";

	// For "GCC_USB30_MASTER_CLK" in HS mode (>=60 MHz)
	assigned-clock-rates = <19200000>, <60000000>;

	// Disable USB3 pipe_clk requirement
	qcom,select-utmi-as-pipe-clk;

	// FIXME: We need a 'extcon_usb2' reference here to actually
	//        detect a plugged in USB cable via interrupts.
	// Some downstream resources:
	// https://github.com/LineageOS/android_kernel_oneplus_msm8998/blob/lineage-16.0/arch/arm/boot/dts/qcom/msm8998.dtsi#L1780
	// https://github.com/LineageOS/android_kernel_oneplus_msm8998/blob/lineage-16.0/arch/arm/boot/dts/qcom/msm-pmi8998.dtsi#L282-L307
	// https://github.com/LineageOS/android_kernel_oneplus_msm8998/blob/lineage-16.0/Documentation/devicetree/bindings/usb/qpnp-pdphy.txt
	//extcon = <&pmi8998_pdphy>;
};

&usb3_dwc3 {
	// Drop "usb1_ssphy" / "usb3-phy" PHY -> we now get a "usb0" interface
	phys = <&qusb2phy>;
	phy-names = "usb2-phy";

	maximum-speed = "high-speed"; // We should only operate at USB 2.0 speeds

	dr_mode = "peripheral"; // Force to peripheral until we have Type-C hooked up
};

&qusb2phy {
	status = "okay";

	vdd-supply = <&vreg_l1a_0p875>;
	vdda-pll-supply = <&vreg_l12a_1p8>;
	vdda-phy-dpdm-supply = <&vreg_l24a_3p075>;
	/delete-property/ nvmem-cells;
};

&wifi {
	status = "okay";

	vdd-0.8-cx-mx-supply = <&vreg_l5a_0p8>;
	vdd-1.8-xo-supply = <&vreg_l7a_1p8>;
	vdd-1.3-rfa-supply = <&vreg_l17a_1p3>;
	vdd-3.3-ch0-supply = <&vreg_l25a_3p3>;
};

/* PINCTRL - board-specific pinctrl */
&blsp1_uart3_on {
	rx {
		/delete-property/ bias-disable;
		/*
		 * Configure a pull-up on 45 (RX). This is needed to
		 * avoid garbage data when the TX pin of the Bluetooth
		 * module is in tri-state (module powered off or not
		 * driving the signal yet).
		 */
		bias-pull-up;
	};

	cts {
		/delete-property/ bias-disable;
		/*
		 * Configure a pull-down on 47 (CTS) to match the pull
		 * of the Bluetooth module.
		 */
		bias-pull-down;
	};
};
